scription\r\n                this.setState({ title: rss.title, items: rss.items })\r\n                this.setState({ content_title: this.state.items.map((e, i) => e.title) })\r\n                this.setState({ content_description: this.state.items.map((e, i) => e.description) })\r\n\r\n\r\n            })\r\n    }\r\n\r\n    // fonction pour récupérer le tableau dans les fils\r\n    passitems()\r\n    {\r\n        if (this.props.getNbItems!= null)\r\n        this.props.getNbItems(this.state.items)\r\n    }\r\n\r\n\r\n    render() {\r\n        return (\r\n            <div style={{marginLeft:'2%',width:'80%' }}>\r\n                {/* tag en dur pour le moment*/}\r\n                <div style={{color:'#440BD4',fontSize:'60%',marginBottom:'0.5%',fontWeight:'bold'}}>#TEAM</div>\r\n                {/* titre */}\r\n                <div style={{marginBottom:'1%',fontWeight:'bold',fontSize:'120%',color:'#346060',fontSize:'90%' }}>\r\n                    {this.state.content_title[this.props.id]}\r\n                </div>\r\n                {/* jour en dur pour le moment */}\r\n                <div style={{color:'#827878',fontSize:'60%',marginBottom:'0.5%'}}>— 1jour</div>\r\n                {/* description à afficher si différent de false*/}\r\n                {this.props.description !==false && <div style={{ color:'#827878',fontSize:'60%'}}>\r\n                    {this.state.content_description[this.props.id]}\r\n                </div>}\r\n                {this.passitems()}\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default MainNews;\r\n","D:\\GIT\\Plateforme_EsportV2\\Front\\src\\Component\\News\\SmallNews.js",["94"],"import React, { Component, useState } from 'react';\r\nimport News from './MainNews';\r\nimport setup from '../../Img/setup.jpg'\r\n\r\nfunction SmallNews() {\r\n\r\n    // initialisation du nombre d'articles\r\n    const [more,setMore] = useState(7);\r\n    const [nbItems,setNbItems] = useState(0); \r\n\r\n    function getNbItems(items)\r\n    {\r\n        // récuperation et initialisation du nb max\r\n        setNbItems(items.length)\r\n    }\r\n    \r\n    function showMore()\r\n    {\r\n        let nb=nbItems-more\r\n        //pour un affichage par 3 choix arbitraire\r\n        if(nb<3)\r\n        {\r\n            // blocage à la taille max du tableau\r\n            setMore(more+(nbItems-more))\r\n        }\r\n        else\r\n        setMore(more+3)\r\n    }\r\n\r\n    // création du tableau\r\n    const articles = [];\r\n    for (let i = 4; i < more; i++) {\r\n        articles.push(\r\n            <>\r\n                <div style={{ marginTop: '1%', marginBottom: '1%', justifyContent: 'center', alignItems: 'center', display: 'flex' }}>\r\n                    <img style={{ width: '20%', borderRadius: 10 }} src={setup} alt=\"article\" />\r\n                    {/* affichage des articles de 4 à more et passage des props id et description */}\r\n                    <News id={i} description={false} getNbItems={(items)=>getNbItems(items)}  />\r\n                </div>\r\n                <hr style={{ backgroundColor: 'grey', marginLeft: '10%', marginRight: '10%', height: \"0.05px\", marginBottom: '1%' }} />\r\n            </>\r\n        )\r\n\r\n    }\r\n    return (\r\n        <div style={{marginLeft:'10%'}}>\r\n            {/*  affichage du tableau */}\r\n            {articles}\r\n            <div style={{justifyContent:'center', alignItems: 'center',display:'flex',marginBottom:'10%',marginTop:'5%'}}>\r\n                {/*button pour ajouter des News*/}\r\n                <button onClick={()=> showMore()} style={{ borderColor: '#440BD4', backgroundColor: '#440BD4', borderWidth: 2, borderStyle: 'solid', borderRadius: 15, padding: 10, alignContent: 'center', color: 'white', fontWeight: 'bold', textAlign: 'center',width:100 }}>Voir plus</button>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default SmallNews;\r\n","D:\\GIT\\Plateforme_EsportV2\\Front\\src\\Pages\\Register.js",["